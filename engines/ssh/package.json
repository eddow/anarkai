{
  "name": "ssh",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "exports": {
    "./src/*": "./src/*",
    "./assets/*": "./assets/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.app.json && tsc -p tsconfig.node.json",
    "preview": "vite preview",
    "check": "tsc -p tsconfig.app.json && tsc -p tsconfig.node.json",
    "biome": "biome check . --fix",
    "biome:unsafe": "biome check --fix --unsafe",
    "test": "vitest run",
    "test:core": "vitest run src/lib/game/job.test.ts src/lib/utils/pathfinding.test.ts src/lib/game/storage/storage.test.ts",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@iconify-icons/material-symbols": "^1.2.58",
    "@iconify-icons/mdi": "^1.2.48",
    "@picocss/pico": "^2.0.6",
    "arktype": "^2.1.22",
    "dockview-core": "^4.7.1",
    "eventemitter3": "^5.0.1",
    "mutts": "workspace:*",
    "npc-script": "workspace:*",
    "omni18n": "workspace:*",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@biomejs/biome": "^2.2.7",
    "@playwright/test": "^1.57.0",
    "@types/babel__core": "^7.20.5",
    "@types/node": "^24.6.2",
    "pixi-stats": "^4.2.0",
    "sass-embedded": "^1.93.3",
    "vite-plugin-babel": "^1.3.2"
  }
}