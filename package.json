{
  "name": "anarkai-monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm -r dev",
    "dev:pounce": "pnpm --filter ssh-pounce dev",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "ownk:test": "./scripts/ownk-test-watch.sh --once",
    "ownk:test:watch": "./scripts/ownk-test-watch.sh --watch",
    "clean:cache": "(pkill -f vitest || true) && find . -type d -name \".vite\" -prune -exec rm -rf {} +"
  },
  "packageManager": "pnpm@10.7.1+sha512.2d92c86b7928dc8284f53494fb4201f983da65f0fb4f0d40baafa5cf628fa31dae3e5968f12466f17df7e97310e30f343a648baea1b9b350685dafafffdf5808",
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "overrides": {
      "vite": "7.3.1",
      "@types/node": "25.0.10"
    }
  },
  "workspaces": [
    "apps/*",
    "engines/*"
  ]
}